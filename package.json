    "ts-node": "10.9.1",
    "zod": "3.21.4",
    "zod-validation-error": "1.3.1"
  },
  "scripts": {
    "start:devnet": "cross-env VITE_APP_ENVIRONMENT=devnet vite",
    "build:devnet": "cross-env VITE_APP_ENVIRONMENT=devnet && tsc && vite build",
    "start:testnet": "cross-env VITE_APP_ENVIRONMENT=testnet vite",
    "build:testnet": "cross-env VITE_APP_ENVIRONMENT=testnet && tsc && vite build",
    "start:mainnet": "cross-env VITE_APP_ENVIRONMENT=mainnet vite",
    "build:mainnet": "cross-env VITE_APP_ENVIRONMENT=mainnet && tsc && vite build",
    "test": "vitest --reporter verbose",
    "test:coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "vite preview",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "format": "prettier --write ./src",
    "check-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@types/js-cookie": "3.0.3",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-helmet": "6.1.6",
    "@types/react-redux": "7.1.25",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-system": "5.1.16",
    "@types/webpack-env": "1.18.1",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-v8": "0.34.1",
    "cypress": "12.17.3",
    "env-cmd": "10.1.0",
    "husky": "^8.0.3",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.0.1",
    "vite": "^4.4.9",
    "vite-plugin-babel-macros": "1.0.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.1",
    "typescript": "5.1.6"
  }
}
